@using MilOficios.Entidades
@{
    ViewBag.Title = "MUDANZAS";
    Random rnd = new Random();
    var user = Session["Usuario"] as Usuario;

    if (user == null)
    {
        Response.Redirect("/Usuario/Login");
    }
}

<div class="container">
    <div class="card">
        <div class="card-body">
            <h3 class="card-title">SERVICIOS DE MUDANZAS</h3>
            <div class="table-responsive">
                <table class="table table-sm table-bordered" id="table">
                    <thead align="center">
                        <tr style="color: white;background-color: #308ee0">
                            <th>#</th>
                            <th>Nombre del Ofertante</th>
                            <th>Fecha Inicio</th>
                            <th>Fecha Fin</th>
                            <th>Descripcion</th>
                            <th>Ver más</th>
                        </tr>
                    </thead>
                    <tbody align="center"></tbody>
                </table>
                <br />

            </div>
        </div>
    </div>
</div>
@{
    Html.RenderPartial("_ModalDetalle");
}

@section scripts{

    <script src="~/Scripts/Home/ListServicios.js?v=@rnd.Next()"></script>
}